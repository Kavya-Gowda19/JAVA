import java.util.Scanner;
public class Patterns3 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=s.nextInt();
		for(int i=0;i<n;i++) {
			System.out.print("#");
		}	
	}
}

