package pack.com;
import java.util.Scanner;
public class Patterns {
	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
  System.out.println("Enter the number");
  int n=s.nextInt();
  for(int i=0;i<=n;i++{
		System.out.println("#");
	}
}
}


